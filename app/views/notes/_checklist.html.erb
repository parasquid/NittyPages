<ul class="whitespace-normal w-full list-[square] px-4">
  <% note.checklist_items.fetch(:todos).each do |todo| %>
    <li class=""><%= todo.fetch(:line) %> [<%= todo.fetch(:number) %>]</li>
  <% end %>
</ul>

<ul class="whitespace-normal w-full list-[square] px-4">
  <% note.checklist_items.fetch(:dones).each do |todo| %>
    <li class="line-through"><%= todo.fetch(:line) %> [<%= todo.fetch(:number) %>]</li>
  <% end %>
</ul>
